{
    "type": "sqlite",
    "database": "src/app/database/database.sqlite",
    "synchronize": true,
    "entities": [
       "src/app/database/model/*.ts"
    ],
    "migrations": [
       "src/app/database/migrations/*.ts"
    ],
    "cli": {
        "migrationsDir": "src/app/database/migrations",
        "entitiesDir": "src/app/database/model"
    }
 }